{
  "name": "track-worker",
  "version": "0.0.0",
  "sideEffects": false,
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "pnpm run '/^(lint:biome|lint:eslint)$/'",
    "lint:biome": "biome check .",
    "lint:eslint": "eslint --ext .ts,.tsx .",
    "lint:fix": "pnpm run '/^(lint:biome|lint:eslint):fix$/'",
    "lint:eslint:fix": "eslint --ext .ts,.tsx . --fix",
    "lint:biome:fix": "biome check . --apply-unsafe",
    "tsc": "tsc",
    "build:vite": "vite build",
    "build:wrangler": "wrangler deploy --dry-run --outdir .wrangler/build",
    "build": "npm run build:vite && npm run build:wrangler",
    "deploy": "wrangler deploy",
    "start": "concurrently -c 'auto'  npm:start:*",
    "start:wrangler": "wrangler dev --test-scheduled --persist-to .wrangler/state",
    "start:vite": "vite",
    "test": "vitest",
    "test:ci": "vitest"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.1",
    "@sentry/tracing": "7.114.0",
    "@tsndr/cloudflare-worker-jwt": "^2.5.3",
    "fast-querystring": "1.1.2",
    "fuzzyset": "^1.0.7",
    "hono": "4.4.2",
    "ofetch": "^1.3.4",
    "toucan-js": "3.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@cloudflare/workers-types": "4.20240529.0",
    "@ctrl/eslint-config-biome": "^2.6.8",
    "@ctrl/eslint-config-react-biome": "^2.2.0",
    "@sentry/types": "7.114.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "5.40.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/fuzzyset": "^1.0.7",
    "@vitejs/plugin-react-swc": "3.7.0",
    "eslint": "8.57.0",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "postcss": "8.4.38",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.23.1",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.12",
    "vitest": "1.6.0",
    "wrangler": "3.58.0"
  },
  "repository": "scttcper/track-worker"
}

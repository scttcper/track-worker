{
  "name": "scrdb-worker",
  "version": "0.0.0",
  "sideEffects": false,
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "pnpm run '/^(lint:biome|lint:eslint)$/'",
    "lint:biome": "biome check .",
    "lint:eslint": "eslint --ext .ts,.tsx .",
    "lint:fix": "pnpm run '/^(lint:biome|lint:eslint):fix$/'",
    "lint:eslint:fix": "eslint --ext .ts,.tsx . --fix",
    "lint:biome:fix": "biome check . --apply",
    "start:wrangler": "wrangler dev --test-scheduled --persist-to .wrangler/state",
    "start:vite": "vite",
    "tsc": "tsc",
    "build:vite": "vite build",
    "build:wrangler": "wrangler deploy --dry-run --outdir .wrangler/build",
    "build": "npm run build:vite && npm run build:wrangler",
    "deploy": "wrangler deploy",
    "start": "concurrently -c 'auto'  npm:start:*",
    "test": "vitest",
    "test:ci": "vitest"
  },
  "dependencies": {
    "@ctrl/video-filename-parser": "5.2.0",
    "@fontsource/inter": "^5.0.17",
    "@hono/zod-validator": "^0.1.11",
    "@js-temporal/polyfill": "^0.4.4",
    "@sentry/tracing": "7.105.0",
    "@tsndr/cloudflare-worker-jwt": "^2.5.2",
    "date-fns": "3.3.1",
    "fast-querystring": "1.1.2",
    "fast-xml-parser": "4.3.5",
    "hono": "4.0.9",
    "ky": "1.2.2",
    "lodash-es": "4.17.21",
    "toucan-js": "3.3.1",
    "type-fest": "4.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@cloudflare/workers-types": "4.20240222.0",
    "@ctrl/eslint-config-react-biome": "^2.0.0",
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "2.1.1",
    "@sentry/types": "7.105.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "5.24.8",
    "@types/lodash-es": "4.17.12",
    "@types/react": "18.2.62",
    "@types/react-dom": "18.2.19",
    "@types/react-highlight-words": "0.16.7",
    "@vitejs/plugin-react-swc": "3.6.0",
    "autoprefixer": "10.4.18",
    "better-sqlite3": "9.4.3",
    "clsx": "2.1.0",
    "concurrently": "8.2.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-highlight-words": "0.20.0",
    "react-router-dom": "6.22.2",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.1.5",
    "vitest": "1.3.1",
    "wrangler": "3.30.1"
  },
  "repository": "scttcper/scrdb-worker"
}
